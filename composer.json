{
    "name": "sgi-software/api-ruc",
    "description": "package para realizar llamada al api de SGI-Software y Dominios para obtener los contribuyentes registrados en la Secretaría de Estado de Tributación/Paraguay",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/SgiSoftware/class/ApiCall.php"
        ],
        "psr-0": {
            "SgiSoftware\\ApiRuc\\": "src"
        }
    },
    "autoload-dev": {
        "files": [
            "src/SgiSoftware/class/ApiCall.php"
        ],
        "psr-0": {
            "SgiSoftware\\ApiRuc\\": "src"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "SgiSoftware\\Publish::config"
        ],
        "post-update-cmd": [
            "SgiSoftware\\Publish::config"
        ]
    },
    "authors": [
        {
            "name": "Alan Laspina",
            "email": "alanjoselaspina@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1|^8"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.0-dev"
        }
    }
}
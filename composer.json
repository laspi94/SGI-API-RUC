{
    "name": "sgi-software/api-ruc",
    "description": "package para realizar llamada al api de SGI-Software y Dominios para obtener los contribuyentes registrados en la Secretaría de Estado de Tributación/Paraguay",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/class/ApiCall.php"
        ],
        "psr-0": {
            "SgiSoftware\\ApiRuc\\": "vendor/sgi-software/api-ruc/src/"
        }
    },
    "autoload-dev": {
        "files": [
            "src/class/ApiCall.php"
        ],
        "psr-0": {
            "SgiSoftware\\ApiRuc\\": "vendor/sgi-software/api-ruc/src/"
        }
    },
    "scripts": {
        "post-install-cmd": [
            "SgiSoftware\\ApiRuc\\Publish::config"
        ],
        "post-update-cmd": [
            "SgiSoftware\\ApiRuc\\Publish::config"
        ]
    },
    "authors": [
        {
            "name": "Alan Laspina",
            "email": "alanjoselaspina@gmail.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": "^7.1|^8",
        "illuminate/support": "^9.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "SgiSoftware\\ApiRuc\\ApiServiceProvider"
            ],
            "branch-alias": {
                "dev-master": "1.0.0-dev"
            }
        }
    }
}